<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjsasc.plm.mapper.DocumentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="documentResultMap" type="com.bjsasc.plm.domain.Document">
        <result column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="status" property="status"/>
        <result column="masterId" property="masterId"/>
    </resultMap>
    <sql id="Base_info_List">

    </sql>
    <select id="history"  resultType="DocumentVersion">
        SELECT * FROM DocumentVersion WHERE versionId IN (SELECT documentId FROM document WHERE documentId = #{documentId} AND versionNo = #{versionNo} )
    </select>


</mapper>